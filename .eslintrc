{ "extends": "eslint-config-airbnb",
  "env": {
    "browser": false,
    "node": false,
    "mocha": false
  },
  "rules": {
    "semi" : 0,
    "quotes" : 0,
    "indent" : 0,
    "no-shadow" : 0,
    "key-spacing" : 0,
    "react/no-multi-comp": 0,
    "import/default": 0,
    "import/no-duplicates": 0,
    "import/named": 0,
    "import/namespace": 0,
    "import/no-unresolved": 0,
    "import/no-named-as-default": 2,
    "comma-dangle": 0,  // not sure why airbnb turned this on. gross!
    "no-console": 0,
    "no-alert": 0,
    "no-unused-vars": 0,
    "space-before-blocks" :0,
    "spaced-comment":0,
    "eol-last" : 0,
    "space-after-keywords" : 0,
    "no-multi-spaces" : 0,
    "new-cap" : 0,
    "padded-blocks" : 0,
    "no-trailing-spaces" : 0,
    "space-infix-ops" : 0,
    "comma-spacing" : 0
  },
  "plugins": [
    "react", "import"
  ],
  "settings": {
    "import/parser": "babel-eslint",
    "import/resolve": {
      moduleDirectory: ["node_modules", "src"]
    }
  },
  "globals": {
    "__DEVELOPMENT__": true,
    "__CLIENT__": true,
    "__SERVER__": true,
    "__DISABLE_SSR__": true,
    "__DEVTOOLS__": true,
    "socket": true,
    "webpackIsomorphicTools": true
  }
}
